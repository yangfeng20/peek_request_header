{
  "name": "Peek Request Header",
  "description": "窥视浏览器每次请求的headers json数据",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon.png",
    "24": "images/icon.png",
    "32": "images/icon.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
